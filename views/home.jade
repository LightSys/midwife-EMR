extends layouts/default_layout

block home
  h1= title

  div.row
    div(class='col-md-6')
      h3(class='charts')= gettext('Prenatal Exams This Week')
      div(id='prenatalThisWeek', data-data=prenatalThisWeekData, data-options=prenatalThisWeekOptions)
    div(class='col-md-6')
      h3(class='charts')= gettext('Prenatal History')
      div(id='prenatalHistory', data-data=prenatalHistoryData, data-options=prenatalHistoryOptions)

  hr
  - if (applicationRevision)
    p= gettext('Application revision: ') + applicationRevision

  p= gettext('Designed for a desktop/laptop.')

