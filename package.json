  "scripts": {
    "test": "mocha",
    "postinstall": "scripts/npm_lifecycle.js",
    "startdefault": "NODE_ENV=development node cluster.js",
    "start": "NODE_ENV=development node cluster.js --config config/config.development.json",
    "watch": "NODE_ENV=development nodemon --ignore client/ --ignore static/ -V -- cluster.js --config config/config.development.json",
    "simple": "NODE_ENV=development nodemon --ignore client/ --ignore static/ -V -- cluster.js --config /home/kurt/work/config.simple.js",
    "build": "gulp",
    "clean": "rm -f static/js/*",
    "build_admin_client_compiler": "(cd client/elm/admin;docker build -f Dockerfile -t midwife-emr-compile-admin-client --no-cache .)",
    "admin_client_dev": "docker run --init --name=midwife-emr-compile-admin-client --rm -v $(pwd)/client/elm/admin:/opt/src -v $(pwd)/static/js:/opt/dist midwife-emr-compile-admin-client:latest",
    "admin_client_prod": "docker run --init --name=midwife-emr-compile-admin-client --rm -v $(pwd)/client/elm/admin:/opt/src -v $(pwd)/static/js:/opt/dist midwife-emr-compile-admin-client:latest webpack --config webpack.prod.js",
    "admin_client_bash": "echo '===== NOTE: NPM script admin_client_dev needs to be running. ====';docker container exec -it midwife-emr-compile-admin-client bash",
    "build_medical_client_compiler": "(cd client/elm/medical;docker build -f Dockerfile -t midwife-emr-compile-medical-client --no-cache .)",
    "medical_client_dev": "docker run --init --name=midwife-emr-compile-medical-client --rm -v $(pwd)/client/elm/medical:/opt/src -v $(pwd)/static/js:/opt/dist midwife-emr-compile-medical-client:latest",
    "medical_client_prod": "docker run --init --name=midwife-emr-compile-medical-client --rm -v $(pwd)/client/elm/medical:/opt/src -v $(pwd)/static/js:/opt/dist midwife-emr-compile-medical-client:latest webpack --config webpack.prod.js",
    "medical_client_bash": "echo '===== NOTE: NPM script medical_client_dev needs to be running. =====';docker container exec -it midwife-emr-compile-medical-client bash",
    "medical_client_tests": "echo '===== NOTE: NPM script medical_client_dev needs to be running. =====';docker container exec -it midwife-emr-compile-medical-client elm-test"
  },
  "jshintConfig": {
    "laxcomma": true
  },
  "author": "Kurt Symanzik",
  "license": "GPLv3"
}
